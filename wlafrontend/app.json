{
  "expo": {
    "name": "WildLifeAlert",
    "slug": "wildlifealert",
    "scheme": "wlawildlifealert",
    "version": "2.6.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to include them in your rescue alert"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
        "bundleIdentifier": "com.wildlifealertusa.wildlifealert",
        "buildNumber": "8",
        "infoPlist": {
          "NSLocationWhenInUseUsageDescription": "WildLifeAlerts uses your location to place the alert on the in-app map, or, if registered as a rescuer, to determine if you are near an alert and should be notified of the situation",
          "NSLocationAlwaysUsageDescription": "WildLifeAlerts uses your location to place the alert on the in-app map, or, if registered as a rescuer, to determine if you are near an alert and should be notified of the situation"
        },
        "config": {
          "googleMapsApiKey": "@iosapikey"
        }
    },
    "android": {
      "versionCode": 6,
      "package": "com.wildlifealertusa.wildlifealert",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "minSdkVersion": 33,
      "permissions": [
        "READ_EXTERNAL_STORAGE", "ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "@androidapikey"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "clerkPublishableKey": "pk_live_Y2xlcmsud2lsZGxpZmVhbGVydHVzYS5jb20k",
      "eas": {
        "projectId": "17a356f2-ec4c-4d59-920f-b77650d9ba44"
      }
    },
    "owner": "jesseye30"
  }
}


